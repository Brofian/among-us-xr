{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.80",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "react": "^18.2.0",
    "react-app-rewired": "^2.2.1",
    "react-dom": "^18.2.0",
    "react-dotenv": "^0.1.3",
    "react-scripts": "5.0.1",
    "sass": "^1.70.0",
    "socket.io-client": "^4.7.4",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@amongusxr/types": "file:../../types"
  },
  "scripts": {
    "dev":    "npm run loadenv && react-dotenv && react-app-rewired start",
    "build":  "npm run loadenv && tsc -p . && react-dotenv && react-app-rewired build",
    "serve":  "npm run loadenv && react-dotenv && serve -s build",
    "loadenv": "export \"$(grep -vE \"^(#.*|\\s*)$\" .env)\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "react-dotenv": {
    "whitelist": [
      "REACT_HTTP_PORT",
      "REACT_APP_DOMAIN",
      "API_HTTP_PORT",
      "API_APP_DOMAIN",
      "LOG_LEVEL"
    ]
  }
}